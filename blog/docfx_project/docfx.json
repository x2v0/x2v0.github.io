{
	"metadata":[
		{
			"src":[
				{
					"files":[
						"src/Fusee/Fusee.sln"
					],
					"exclude":[
						"**/bin/**",
						"**/obj/**"
					]
				}
			],
			"dest":"api",
			"filter":"filterConfig.yml",
			"disableGitFeatures":false,
			"disableDefaultFilter":false
		}
	],
	"build":{
		"content":[
			{
				"files":[
					"api/**.yml",
					"api/index.md",
					"examples/**.md",
					"examples/toc.yml",
					"getting-started/**.md",
					"getting-started/toc.yml",
					"wiki/**.md",
          "wiki/toc.yml",
          "toc.yml",
					"*.md"
				],
				"exclude":[
					"wiki/_*"
				]
			}
		],
		"resource":[
			{
				"files":[
					"examples/images/**",
					"examples/rocket/**",
					"examples/picking/**",
					"getting-started/images/**",
					"wiki/images/**",
					"images/**"
				]
			}
		],
		"overwrite":[
			{
				"files":[
					"apidoc/**.md"
				],
				"exclude":[
					"obj/**",
					"docs/**"
				]
			}
		],
		"dest":"../docs",
		"globalMetadataFiles":[],
		"fileMetadataFiles":[],
		"template":[
			"templates/UnityDocFX"
		],
		"postProcessors":[],
		"markdownEngineName":"markdig",
		"noLangKeyword":false,
		"keepFileLink":false,
		"cleanupCacheHistory":false,
		"disableGitFeatures":false,
		"globalMetadata": {
			"_disableContribution": true,
		}
	}
}