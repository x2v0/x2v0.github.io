    <bpmn:userTask id="Activity_DeclarationCheckForm" name="проверка формы декларации">
      <bpmn:documentation>&lt;p&gt;&lt;span style="font-size: 16px;font-family: Calibri, sans-serif;"&gt;&lt;strong&gt;2.&lt;/strong&gt; Бюджетным учреждением&lt;/span&gt;&lt;span style="font-size: 16px;"&gt;&lt;br&gt;&lt;/span&gt;&lt;span style="font-size: 16px;font-family: Calibri, sans-serif;"&gt;рассматривается декларация, поданная правообладателем объекта недвижимости&lt;/span&gt;&lt;span style="font-size: 16px;"&gt;&lt;br&gt;&lt;/span&gt;&lt;span style="font-size: 16px;font-family: Calibri, sans-serif;"&gt;(далее - заявитель), или его представителем, по &lt;/span&gt;&lt;a href="#Par81" target="_self"&gt;&lt;span style="color: windowtext;font-size: 16px;font-family: Calibri, sans-serif;"&gt;форме&lt;/span&gt;&lt;/a&gt;&lt;span style="font-size: 16px;font-family: Calibri, sans-serif;"&gt;,&lt;/span&gt;&lt;span style="font-size: 16px;"&gt;&lt;br&gt;&lt;/span&gt;&lt;span style="font-size: 16px;font-family: Calibri, sans-serif;"&gt;предусмотренной приложением N 2 к настоящему приказу&lt;/span&gt;&lt;span style="font-size: 16px;"&gt; &lt;/span&gt;&lt;/p&gt;</bpmn:documentation>
      <bpmn:incoming>Flow_1pf2dhc</bpmn:incoming>
      <bpmn:outgoing>Flow_04qvo40</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="Activity_DeclarationCheckRights" name="проверка доверенности или иного подтверждающего полномочия представителя заявителя документа">
      <bpmn:documentation>&lt;p style="text-align:justify;"&gt;&lt;strong&gt; 3)&lt;/strong&gt; доверенности или иного подтверждающего&lt;br&gt;полномочия представителя заявителя документа, удостоверенных в соответствии с&lt;br&gt;законодательством Российской Федерации.&amp;nbsp;&amp;nbsp;&lt;/p&gt;</bpmn:documentation>
      <bpmn:incoming>Flow_1j5ay1a</bpmn:incoming>
