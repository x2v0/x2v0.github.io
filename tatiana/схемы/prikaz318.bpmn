      <bpmn:outgoing>Flow_1bqf21f</bpmn:outgoing>
      <bpmn:timerEventDefinition id="TimerEventDefinition_1dj3s0a">
        <bpmn:timeDate xsi:type="bpmn:tFormalExpression">день</bpmn:timeDate>
      </bpmn:timerEventDefinition>
    </bpmn:intermediateCatchEvent>
